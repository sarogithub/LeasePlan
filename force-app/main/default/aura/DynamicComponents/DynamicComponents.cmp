<aura:component>
    <aura:attribute name="ComponentName" type="String" access="public"/>
    <aura:attribute name="obj" type="Object" access="public"/>
    <aura:attribute name="FieldName" type="String" access="public"/>
    <aura:attribute name="AuraId" type="String" access="public"/>

    <aura:handler name="init" value="{!this}" action="{!c.doInit}"/>

    {!v.body}
</aura:component>	
